{
    "compilerOptions": {
        "target": "es2024",
        "module": "nodenext",
        "outDir": "webpack-config-out",
        "rootDir": "./webpack-config/",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "strict": true,
        "types": [
            "node",
            "webpack",
            "webpack-dev-server"
        ],
        "lib": [ "ES2024" ]
    },
    "include": [ "./webpack-config/*" ]
}